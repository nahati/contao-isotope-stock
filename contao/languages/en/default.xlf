                <target>The maximum available quantity for a variant of "%s" is %s items</target>
            </trans-unit>
            <trans-unit id="ERR.adaptCart">
                <target>Adapt the "%s" in cart by their quantity!</target>
            </trans-unit>
            <trans-unit id="ERR.productHasChanged">
                <target>"%s" has changed in the meanwhile, please start again!</target>
            </trans-unit>

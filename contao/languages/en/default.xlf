        <body>
            <trans-unit id="ERR.overbought">
                <target>The product "%s" has been overbought! We will come back to you soon."
                    product-type</target>
            </trans-unit>
            <trans-unit id="ERR.inventoryStatusInactive">
                <target>The inventory_status has not been activated for this products "%s"
                    product-type</target>
            </trans-unit>
            <trans-unit id="ERR.productNotAvailable">
                <target>This product "%s" is not available</target>
            </trans-unit>
            <trans-unit id="ERR.productOutOfStock">
                <target>The product "%s" is currently out of stock</target>
            </trans-unit>
            <trans-unit id="ERR.quantityNotAvailable">
                <target>The maximum available quantity for "%s" is %s items</target>
            </trans-unit>
            <trans-unit id="ERR.parentQuantityNotAvailable">
                <target>The maximum available quantity for "%s" is %s items</target>
            </trans-unit>

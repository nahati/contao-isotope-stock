                <target>Update of inventory_status failed for product "%s".</target>
            </trans-unit>
            <trans-unit id="ERR.quantityUpdateFailed">
                <target>Update of quantity failed for product "%s".</target>
            </trans-unit>
            <trans-unit id="MSC.checkProduct">
                <target>The product "%s" has been marked "reserved" and hence not available. Please
                    check!</target>
            </trans-unit>
            <trans-unit id="MSC.1">
                <target>unsellable</target>
            </trans-unit>
            <trans-unit id="MSC.2">
                <target>available</target>
            </trans-unit>
            <trans-unit id="MSC.3">
                <target>reserved</target>
            </trans-unit>
